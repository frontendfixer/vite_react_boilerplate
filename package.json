{
  "name": "@frontendfixer/vite-react-boilerplate",
  "version": "1.0.1",
  "description": "React Boilerplate using Vite , Prettier, Eslint, Stylelint, and SASS ",
  "keywords": [
    "react",
    "vite",
    "react template",
    "vite react template",
    "javascript",
    "typescript",
    "config"
  ],
  "main": "index.html",
  "repository": "git@github.com:frontendfixer/vite_react_boilerplate.git",
  "author": "Lakshmikanta <frontendfixer@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --host",
    "lint:eslint": "eslint . --ext .js,.jsx",
    "fix:eslint": "eslint --fix  . --ext .js,.jsx",
    "lint:stylelint": "stylelint ./src/**/*.{css,scss,jsx}",
    "fix:stylelint": "stylelint --fix  ./src/**/*.{css,scss,jsx}",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@frontendfixer/eslint-config-react": "^1.0.4",
    "@namics/stylelint-bem": "^8.1.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.7",
    "sass": "^1.62.0",
    "stylelint": "^15.6.0",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard-scss": "^8.0.0",
    "stylelint-rem-over-px": "^1.0.0",
    "stylelint-scss": "^4.6.0",
    "svgo": "^3.0.2",
    "terser": "^5.17.4",
    "vite": "^4.3.0",
    "vite-plugin-image-optimizer": "^1.1.3",
    "vite-plugin-svgr": "^2.4.0"
  }
}
